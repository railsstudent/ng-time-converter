{
    "extends": "../tslint.json",
    "rules": {
        "rxjs-add": { "severity": "error" },
        "rxjs-no-unused-add": { "severity": "error" },
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
